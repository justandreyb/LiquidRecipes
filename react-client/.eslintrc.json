{
  "env": {
    "browser": true,
    "jest": true
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "arrow-parens": ["error", "always"],
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": 0,
    "comma-dangle": [1,"never"],
    "no-dupe-args": 1,
    "no-dupe-keys": 1,
    "no-empty": 1,
    "no-extra-parens": [1,"all"],
    "no-extra-semi": 1,
    "complexity": [1,3],
    "consistent-return": 1,
    "curly": [1,"multi"],
    "default-case": 1,
    "eqeqeq": [1,"smart"],
    "no-unused-vars": 0,
    "brace-style": [1,"stroustrup",{"allowSingleLine":true}],
    "camelcase": [1,{"properties":"always"}],
    "comma-spacing": [1,{"after":true}],
    "computed-property-spacing": [1,"always"],
    "eol-last": 1,
    "indent": [1,2],
    "key-spacing": [1,{"align":"colon","afterColon":true}],
    "lines-around-comment": [1,{"beforeBlockComment":true,"afterBlockComment":true,"beforeLineComment":true,"afterLineComment":true,"allowBlockStart":true}],
    "no-mixed-spaces-and-tabs": [1,"smart-tabs"],
    "no-trailing-spaces": [1,{"skipBlankLines":true}],
    "quotes": [1,"double",{"avoidEscape": true}],
    "semi-spacing": 1,
    "require-yield": 1
  }
}
